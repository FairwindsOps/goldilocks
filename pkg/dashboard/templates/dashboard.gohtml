<!DOCTYPE html>
<html lang="en-US">
  <head>
    {{ template "head" . }}
    
    <script>
      window.summaryData = {{ .JSON }};
    </script>
  </head>

  <body>
    {{ template "navbar" . }}
    <div class="main-content">
      {{ template "preamble" . }}
      {{ range $nsName, $nsSummary := .Data.Namespaces }}
        {{ template "namespace" $nsSummary }}
      {{end}}
    </div>
    {{ template "footer" . }}
  </body>
</html>
